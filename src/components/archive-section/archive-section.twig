                    <div class="col-sm-8 col-12">
                      <label class="header-section__input">
                        <input type="text" placeholder="Hledat v článcích..." />
                      </label>
                    </div>
                    <div class="col-sm-4 col-12">
                      <button class="btn">Hledat</button>
                    </div>
                  </div>
